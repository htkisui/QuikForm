<vc:form-row form-view-model='new FormViewModel(){ Id = 2, Title = "Titre 2", Description = "Description 2", CreatedAt = DateTime.Parse("01/01/2024"), PublishedAt = DateTime.Parse("08/01/2024"), UpdatedAt = DateTime.Parse("08/01/2024"), ClosedAt = DateTime.Parse("25/01/2024")}' />
